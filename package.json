{
  "name": "mwaa-airflow",
  "version": "0.1.0",
  "packageManager": "pnpm@10.7.1",
  "bin": {
    "mwaa-airflow": "bin/mwaa-airflow.js"
  },
  "scripts": {
    "ci": "pnpm run build && pnpm run lint && pnpm run test && cdk synth",
    "build": "tsc",
    "lint": "eslint && ruff check",
    "test": "vitest run && export AIRFLOW_HOME=$(pwd) && airflow db migrate && pytest --ignore=cdk.out",
    "test:update": "vitest run -u"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.118.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@types/node": "^22.7.4",
    "aws-cdk": "^2.118.0",
    "eslint": "^9.37.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vitest": "^3.1.1"
  }
}
